VPNNAME='OpenVPN'

HASCONFIG='false'
ISCONFIGURED='false'

case "$1" in
	'CONFIGURE')
		return 0
		;;
	'START')
		return 1
		;;
	'STOP')
		return 0
		;;
	'STATE')
		return 1
		;;
esac

# IPsec VPN script
#
# https://en.wikipedia.org/wiki/IPsec
#
# Rogier van Staveren, January 2019, placeholder for future implementation

VPNNAME='IPsec'

HASCONFIG='false'
ISCONFIGURED='false'

case "$1" in
	'CONFIGURE')
		return 0
		;;
	'START')
		return 1
		;;
	'STOP')
		return 0
		;;
	'STATE')
		return 1
		;;
esac
